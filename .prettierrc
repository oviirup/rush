{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "arrowParens": "always",
  "importOrder": ["<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "<TYPES>"],
  "plugins": [
    "prettier-plugin-imports",
    "prettier-plugin-jsdoc",
    "prettier-plugin-pkg"
  ],
  "quoteProps": "consistent",
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all"
}
